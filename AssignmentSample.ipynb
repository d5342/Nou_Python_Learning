   "source": [
    "# 定義一個包含數字的列表\n",
    "numbers = [1, -2, 3, 4, -5, 6, 0, 7]\n",
    "\n",
    "# 初始化計數器\n",
    "count = 0\n",
    "\n",
    "# 遍歷列表中的每個數字\n",
    "for num in numbers:\n",
    "    if num > 0:  # 檢查數字是否為正整數\n",
    "        count += 1  # 如果是，計數器加一\n",
    "\n",
    "# 輸出正整數的數量\n",
    "print(\"正整數的數量:\", count)\n"
   ]
